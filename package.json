{
  "name": "bugsnag-sourcemap-webpack-plugin",
  "version": "0.0.1",
  "description": "A Webpack plugin to upload sourcemaps to Bugsnag after build",
  "main": "index.js",
  "scripts": {
    "build": "babel src/BugsnagSourceMapPlugin.js -o index.js",
    "test": "NODE_PATH='src' ava",
    "lint": "eslint src/"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ]
  },
  "keywords": [
    "webpack",
    "plugin",
    "bugsnag",
    "sourcemap",
    "source-map"
  ],
  "homepage": "https://github.com/bakunyo/bugsnag-sourcemap-webpack-plugin",
  "bugs": "https://github.com/bakunyo/bugsnag-sourcemap-webpack-plugin/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bakunyo/bugsnag-sourcemap-webpack-plugin"
  },
  "author": "bakunyo <izuta.hiroyuki@gmail.com> (https://github.com/bakunyo)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "ava-spec": "^1.0.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0"
  },
  "dependencies": {
    "async": "^2.1.4",
    "superagent": "^3.3.1"
  }
}
